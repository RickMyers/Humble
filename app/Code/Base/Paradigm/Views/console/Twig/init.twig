<style type="text/css">
    #paradigmConsole {
        background-color: black; color: #0f0; font-family: monospace, serif; font-size: .9em; margin-left: 0px; 
        white-space: pre-wrap; overflow: hidden; text-align: left; position: relative
    } 
    #paradigm_console {
        overflow-y: auto;
    }
