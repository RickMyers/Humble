<apps>
    <app>
        <name>&&MODULE&& Admin App</name>
        <description>Default Administration App for &&PROJECT&&</description>
        <icon>
            <text>&&NAMESPACE&& Tool</text>
            <image>
                <desktop>/images/&&NAMESPACE&&/&&NAME&&_app_icon.png</desktop>
                <minimized>/images/&&NAMESPACE&&/&&NAME&&_app_icon.png</minimized>
            </image>
        </icon>
        <start>
            <uri>/&&NAMESPACE&&/admin/&&NAME&&</uri>
        </start>
    </app>
</apps>

