                    <uniqueid />
                    <timestamp alias="datetime" />
                    <datetime />
                    <time />
                    <datestamp alias="date" />
                    <date />
                    <currentyear />
                    <currentmonth />
                    <currentday />
                    <currentdayofweek />
                </keywords>
            </default>
            <trim     required="false" purpose="" default="" />
            <upper    required="false" purpose="" default="" conflicts="lower" />
