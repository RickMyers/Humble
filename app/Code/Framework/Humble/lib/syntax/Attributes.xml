        <attributes>
            <assign    required="true"   purpose="" default="" />
            <var       required="true"   purpose="" default="" />
        </attributes>
    </application>
    <project taxonomy="metadata">
        <attributes>
            <assign    required="true"   purpose="" default="" />
            <var       required="true"   purpose="" default="" />            
        </attributes>        
    </project>
