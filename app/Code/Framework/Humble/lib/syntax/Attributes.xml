                    <xml />
                    <json />
                    <javascript alias="js" />
                    <pdf />
                    <yaml alias="yml" />
                    <text />
                </values>
            </output>
            <disposition required="false"  purpose="" default="" />
            <filename   required="false"  purpose="" default="" />
            <input      required="false"  purpose="" default="">
