{
    "form":
    {
            "id":           "login-form",
            "drawme":       false,
            "method":       "post",
            "style":        "",
            "onenterkey":   "send"
    },
    "fields":
    [
        {
            "active":       true,
            "id":           "user_name",
            "longname":     "Your Login Name",
            "type":         "text",
            "required":     true,
            "force":        true,
            "style":        "padding: 2px; border-radius: 3px; border: 1px solid transparent"
        },
        {
            "active":       true,
            "id":           "user_password",
            "longname":     "Your password",
            "type":         "password",
            "required":     true,
            "force":        true,
            "style":        "padding: 2px; border-radius: 3px; border: 1px solid transparent"
        },
        {
            "active":	true,
            "id":           "login-submit",
            "longname":     "Submit Button",
            "type":         "button",
            "classname":    "",
            "style":        "",
            "onclick":      () => {
                if (Edits['login'].validate()) {
                    $E("humble-login-form").submit();
                }
            }
        }
    ]
}